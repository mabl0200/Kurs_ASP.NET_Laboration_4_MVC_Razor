@model Book

<h1>Bok lånad</h1>
<div class="thumbnail">
    <img src="@Model.ImageThumbnail" alt="Book Image" />
    <div class="caption-full">
        <h1>@Model.Name</h1>
        <h3 class="pull-right">@Model.Author</h3>
        <h4>@Model.IsAvailable</h4>
        <h3 class="#">@Model.ISBN</h3>
    </div>
</div>